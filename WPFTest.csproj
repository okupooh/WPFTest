    <PackageReference Include="Microsoft.Toolkit" Version="*"/>
    <PackageReference Include="Microsoft.Toolkit.Wpf.UI.Controls.WebView" Version="*"/>
    <PackageReference Include="Microsoft.Web.WebView2" Version="*"/>
    <Content Include=".\Contents\**\*" LinkBase="Contents" CopyToOutputDirectory="PreserveNewest"/>
  </ItemGroup>
</Project>